{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "color: lighten(color(gray-dark), 20%)",
          "color: lighten(color(gray-dark), 10%)",
          "text-decoration-skip-ink",
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "disown-opener": "off"
  }
}